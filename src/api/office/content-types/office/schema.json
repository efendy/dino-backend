{
  "kind": "collectionType",
  "collectionName": "offices",
  "info": {
    "singularName": "office",
    "pluralName": "offices",
    "displayName": "Office"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "site": {
      "type": "relation",
      "required": true,
      "relation": "oneToOne",
      "target": "api::site.site"
    },
    "title": {
      "type": "string",
      "required": true
    },
    "subtitle": {
      "type": "string"
    },
    "contact": {
      "type": "string"
    },
    "is_active": {
      "type": "boolean",
      "default": false
    },
    "has_promo": {
      "type": "boolean",
      "default": false
    },
    "has_whatsapp": {
      "type": "boolean",
      "default": false
    },
    "address": {
      "type": "component",
      "component": "global.shop-address",
      "repeatable": false,
      "required": true
    },
    "operation_info": {
      "type": "component",
      "component": "global.operation-info",
      "repeatable": true
    },
    "tags": {
      "type": "json",
      "required": true
    },
    "shop_images": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files"
      ]
    }
  }
}
